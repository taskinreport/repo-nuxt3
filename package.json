  "dependencies": {
    "@nuxtjs/i18n": "^9.2.1",
    "isomorphic-dompurify": "^2.26.0",
    "nuxt": "^3.15.4",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.13.1"
  }
}
